{
  "family": "petbulance-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "5120",
  "executionRoleArn": "arn:aws:iam::276995858259:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::276995858259:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "petbulance",
      "image": "276995858259.dkr.ecr.ap-northeast-2.amazonaws.com/petbulance:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/petbulance-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
